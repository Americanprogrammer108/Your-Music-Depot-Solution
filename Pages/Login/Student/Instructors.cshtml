@page
@model Your_Music_Depot_Application_DBAS6201.Pages.Login.Student.InstructorsModel
@{
}
<nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
    <div class="container">
        <img src="~/Images/CompanyLogo.png" height="100" />
        <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
            <ul class="navbar-nav flex-grow-1">
                <li class="nav-item">
                    <a class="nav-link text-dark" asp-area="" asp-page="/Login/Student/Dashboard">Home</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link text-dark" asp-area="" asp-page="/Login/Student/Instructors">Instructors</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link text-dark" asp-area="" asp-page="/Login/Student/Scheduling">Scheduling</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link text-dark" asp-area="" asp-page="/Login">Logout</a>
                </li>
            </ul>
        </div>
    </div>
</nav>

<p>You will find all your instructors that you are registered for</p>

<table>
    <thead>
        <tr>
            <th>Name</th>
            <th>Instrument category</th>
            <th>Availablility</th>
            <th>Email</th>
            <th>Phone</th>
            <th>Phone</th>
        </tr>

    </thead>
    @foreach (var item in Model.Instructors)
    {
        <tbody>
            <tr>
                <td> @item.FName @item.LName</td>
                <td> @item.PhoneNumber</td>
                <td> @item.Email</td>
                <td> @item.day</td>
                <td> @item.start</td>
                <td> @item.end </td>
            </tr>
        </tbody>
    }

</table>